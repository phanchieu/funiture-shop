laceholder=\"Email\">\r\n                      <input type=\"phone\" placeholder=\"Số điện thoại\">\r\n                      <input type=\"address\" placeholder=\"Địa chỉ\">\r\n                      <select name=\"city\" id=\"city\">\r\n                        <option>Tỉnh/Thành</option>\r\n                        <option v-for=\"(city,index) in cities\" :key=\"index\" :value=\"city.codename\">{{city.name}}</option>\r\n                      </select>\r\n                      <select name=\"district\" id=\"district\">\r\n                        <option disabled>Quận/Huyện</option>\r\n                        <option v-for=\"(district,index) in districts\" :key=\"index\" :value=\"district.codename\">{{district.name}}</option>\r\n                      </select>\r\n                      <input type=\"address\" placeholder=\"Phường/Xã\">\r\n                      <textarea name=\"content\" id=\"\" rows=\"10\" placeholder=\"Ghi chú\"></textarea>\r\n                     </div>\r\n                      <div class=\"payment_methods\">\r\n                        <div class=\"choose\">\r\n                          <input type=\"radio\" name=\"payment_methods\" id=\"1\"> <label for=\"1\">Thanh toán chuyển khoản</label>\r\n                        </div>\r\n                        <div class=\"choose\">\r\n                          <input type=\"radio\" name=\"payment_methods\" id=\"2\"> <label for=\"2\">Thanh toán khi nhận hàng (COD)</label>\r\n                        </div>\r\n                      </div>\r\n                    </form>\r\n              </div>\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data(){\r\n    return{\r\n        cities:[],\r\n        districts:[],\r\n        \r\n    }\r\n  },\r\n  methods:{\r\n    getCities(){\r\n      axios\r\n      .get('https://provinces.open-api.vn/api/')\r\n      .then((response) => {\r\n      this.cities = response.data\r\n      console.log(this.cities)\r\n      })\r\n    },\r\n    getDistricts(){\r\n      axios\r\n      .get('https://provinces.open-api.vn/api/d')\r\n       .then((response) => {\r\n      this.districts = response.data\r\n      console.log(this.districts)\r\n      })\r\n    }\r\n  },\r\n  created(){\r\n    this.getCities();\r\n    this.getDistricts();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container{\r\n  max-width: 1140px;\r\n}\r\n.Buyer_Information{\r\n  padding: 20px 0;\r\n}\r\n</style>"]}]}